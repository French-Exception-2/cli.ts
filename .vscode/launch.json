{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell bin/Init",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/bin/Init.ps1",
            "cwd": "${workspaceFolder}/tmp"
        },
        {
            "name": "Python : Docker Compose PS",
            "type": "python",
            "request": "launch",
            "program": "C:/Python39/Scripts/docker-compose-script.py",
            "console": "integratedTerminal",
            "args": ["--context", "c0", "ps"],
            "cwd": "${workspaceFolder}/tmp",
            "env": {"COMPOSE_PARAMIKO_SSH":"0"}
        },
        {
            "name": "Python: Attacher",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ]
        },
        {
            "name": "Current TS File",
            "type": "node",
            "request": "launch",
            "args": [
                "${relativeFile}"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register",
                "-r",
                "source-map-support/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
        },
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/watch.ts",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "vagrant:init --status --instance 1 --group \"Test\" --provider \"virtualbox\"",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "vagrant:init",
                "--status",
                "--instance",
                "1",
                "--group",
                "Test",
                "--provider",
                "virtualbox"
            ]
        },
        {
            "name": "vagrant:machine-type:add 1",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "vagrant:machine-type:add",
                "--name",
                "'test'",
                "--vcpus",
                "4",
                "--cpucap",
                "100",
                "--os_type",
                "Debian_64",
                "--os_version",
                "10.8.0",
                "--ram_mb",
                "2048",
                "--vram_mb",
                "8",
                "--no-3d",
                "--pagefusion",
                "--no-gui",
                "--provider",
                "virtualbox",
                "--enabled",
                "--box",
                "debian/contrib-buster64"
            ]
        },
        {
            "name": "vagrant:machine-type:provision:add 1",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "vagrant:machine-type:provision:add",
                "--name",
                "'test'",
                "--type",
                "'test'",
                "--env",
                "'TEST_VAR1=testvalue1'",
                "--env",
                "'TEST_VAR2=testvalue2'",
            ]
        },
        {
            "name": "vagrant:machine:add 1",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "vagrant:machine:add",
                "--name",
                "'test'",
                "--type",
                "'test'",
                "--vcpus",
                "2",
                "--vram_mb",
                "4096",
                "--hostname_pattern",
                "vdi-#{VAGRANT_INSTANCE}-#{NAME}-#{INSTANCE}",
                "--instances",
                "3",
                "--primary"
            ]
        },
        {
            "name": "vagrant:machine:provision:add 1",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "vagrant:machine:provision:add",
                "--machine-name",
                "'test'",
                "--name",
                "'test.provision.configure'",
                "--env",
                "'TEST_VAR1=value1'",
                "--env",
                "'TEST_VAR2=value2'"
            ]
        },
        {
            "name": "vagrant:machine:up dev 1",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "vagrant:machine:up",
                "--name",
                "infra",
            ]
        },
        {
            "name": "vagrant:machine:ssh-config test 1",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "vagrant:machine:ssh-config",
                "--name",
                "test",
                "--instance",
                "0"
            ]
        },
        {
            "name": "config:get test",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "config:get",
                "--key",
                "nodes-types"
            ]
        },
        {
            "name": "config:dump test",
            "program": "${workspaceFolder}/bin/frenchex2",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/tmp",
            "type": "pwa-node",
            "args": [
                "config:dump"
            ]
        }
    ]
}