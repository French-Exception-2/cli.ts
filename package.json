{
  "name": "@frenchex2/cli.ts",
  "version": "1.0.0",
  "description": "Development environment for @frenchex2 products",
  "main": "src/cli.ts",
  "scripts": {
    "dev:build": "npx tsc",
    "dev:watch": "npx tsc -w",
    "package:create": "npx lerna create",
    "test:unit": "npx mocha --recursive -r ts-node/register --extension ts --jobs 4 --parallel test",
    "test:watch": "npx nodemon -e ts -w 'src/' -w 'test/' -x 'npm run test:unit'"
  },
  "directories": {
    "bin": "bin"
  },
  "keywords": [
    "dev",
    "env",
    "virtual",
    "iac"
  ],
